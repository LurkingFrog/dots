{
  "editor.formatOnSave": true,
  "editor.renderIndentGuides": false,
  // "FSharp.logLanguageServiceRequestsOutputWindowLevel": "DEBUG",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.bs.js": true,
    "**/CVS": true,
    "**/node_modules/": true,
    "**/packages": true
  },
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  // "gitlens.historyExplorer.enabled": true,
  // "java.home": "/usr/lib/jvm/java-8-openjdk-amd64/",
  // "[scala]": {},
  "gitlens.keymap": "chorded",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "javascript.validate.enable": false,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 120,
  "search.exclude": {
    "**/.git": true,
    "**/bin": true,
    "**/bower_components": true,
    "**/build": true,
    "**/node_modules": true,
    "**/obj": true,
    "**/packages": true,
    "**/paket-files": true
  },
  "trailing-spaces.trimOnSave": true,
  "window.newWindowDimensions": "inherit",
  "window.openFoldersInNewWindow": "on",
  "window.zoomLevel": 0,
  "files.associations": {
    ".eslintrc": "jsonc",
    "*.json": "jsonc"
  },
  // "scalaLanguageServer.logLevel": "INFO",
  // "scalaLanguageServer.heapSize": "2048M",
  "breadcrumbs.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.enabled": false,
  "editor.rulers": [110],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "window.restoreWindows": "all",
  "rust-client.updateOnStartup": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)",
    "reason"
  ],
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust.all_features": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "reason_language_server.format_width": 110,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "workbench.startupEditor": "newUntitledFile",
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "workbench.editor.labelFormat": "short",
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.general.tags": ["XXX", "TODO", "RESEARCH", "HACK", "FIXME", "BUG", "THINK"],
  "module-templates.engine": "handlebars",
  "module-templates.templates": [],
  "cSpell.userWords": [
    "Eeyo",
    "Fogelson",
    "Opam",
    "adhd",
    "adhd devs",
    "creds",
    "denormalized",
    "descendents",
    "devs",
    "dfogelson",
    "dotenv",
    "excel",
    "executables",
    "fooding",
    "github",
    "github's",
    "gmail",
    "json",
    "microservice",
    "microservices",
    "posix",
    "postgres",
    "queryable",
    "replicant",
    "replicants",
    "routable",
    "rustc",
    "rustfmt",
    "rustup",
    "semver",
    "serde",
    "struct",
    "structs",
    "tablular",
    "upsert",
    "uuid",
    "wasm"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "**/target"
  ],
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "reason_language_server.show_debug_errors": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
