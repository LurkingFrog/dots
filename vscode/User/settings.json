    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "**/target"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "dune.path": "/home/dfogelson/.opam/default/bin/dune",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[reason]": {
    "editor.defaultFormatter": "jaredly.reason-vscode"
  },
  "terminal.integrated.environmentChangesIndicator": "on",
  "reason_language_server.per_value_codelens": true,
  "json.schemas": [
    {
      "fileMatch": ["bsconfig.json"],
      "url": "file:///home/dfogelson/Foundry/rescript-compiler/docs/docson/build-schema.json"
    }
  ],
  "[ocaml]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "jaredly.reason-vscode"
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "files.trimTrailingWhitespace": true,
  "gitlens.blame.ignoreWhitespace": true,
  "editor.minimap.maxColumn": 100,
  "editor.wordWrapColumn": 100,
  "editor.minimap.enabled": false,
  "debug.console.wordWrap": false,
  "rewrap.wrappingColumn": 100,
  "rewrap.wholeComment": true,
  "rewrap.autoWrap.enabled": true,
  "rewrap.reformat": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": "{{put-password-here}}"
    }
  ],
  "grammarly.userWords": ["yewzy", "Yewzy"],
  "grammarly.severity": {},
  "grammarly.showExamples": true,
  "grammarly.domain": "technical",
  "rust-client.revealOutputChannelOn": "warn",
  "rust.build_on_save": true
}
